pipeline {
    agent any 
    
    stages{
        stage('login docker') {
            steps {
                script {
                    sh 'docker login -u nattawat2345 -p dckr_pat_EtOOiCJ1dedZ9GSJ1E8buzC15_A'
                }
            }
        }
        stage('docker pull image') {
            steps {
                script {
                    sh 'docker pull nattawat2345/kpun-be:lastest'
                    sh 'docker pull nattawat2345/kpun-fe:lastest'
                }

            }
        }
        stage('docker run image') {
            steps {
                script {
                    sh 'docker run nattawat2345/kpun-be:lastest'
                    sh 'docker run nattawat2345/kpun-fe:lastest'
                }
            }
        }
    }
}